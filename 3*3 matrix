#include<stdio.h>
#include<conio.h>
int main()
{
int num[20][20],i,j,sum=0,sum1=0,n;
printf("Enter Number of Rows and Colomns in a Matrix");
scanf(%d",&n);
printf("Enter Elements of Matrix ...\n");
for(i=0;i<n;i++)
{
  for(j=0;j<n;j++)
  {
  printf("Enter elements in pocket [%d] [%d] \t",i,j);
  scanf("%d",&num[i] [j]);
  }
  }
  printf("matrix is .....\n");
  for(i=0;i<n;i++)
  {
  for(j=0;j<n;j++)
  {
  print("%d\t,num[i][j]);
  }
  printf("\n");
  }
  getch()
  }
  
